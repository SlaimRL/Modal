					$('#money-form').submit(function(event){
													$('.modal').removeClass("animate-modal");
							$('.modal-content').removeClass("animate-modal-content");
							event.stopPropagation();
							modal('#not-logged', '#not-logged-content');
							return false;
												});

					$('#promo-form').submit(function(event){
													$('.modal').removeClass("animate-modal");
							$('.modal-content').removeClass("animate-modal-content");
							event.stopPropagation();
							modal('#not-logged', '#not-logged-content');
							return false;
